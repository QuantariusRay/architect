{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc",
    "types": []
  },
  "include": [
    "src/**/*.ts"
  ],
  "angularCompilerOptions": {
    "annotationsAs": "static fields",
    "annotateForClosureCompiler": true
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
